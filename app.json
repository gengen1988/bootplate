{
    "name": "Bootplate",
    "indexHtmlPath": "index.html",
    "url": null,
    "js": [
        {
            "path": "cordova.js",
            "remote": true
        },
        {
            "path": "touch/sencha-touch.js",
            "x-bootstrap": true
        },
        {
            "path": "bootstrap.js",
            "x-bootstrap": true
        },
        {
            "path": "app.js",
            "bundle": true,
            "update": "delta"
        }
    ],
    "css": [
        {
            "path": "resources/css/app.css",
            "update": "delta"
        },
        {
            "path": "resources/css/chart.css"
        }
    ],
    "appCache": {
        "cache": [
            "index.html"
        ],
        "network": [
            "*"
        ],
        "fallback": []
    },
    "resources": [
        "config.xml",
        "resources/images",
        "resources/icons",
        "resources/startup",
        "runtime"
    ],
    "ignore": [
        "\\.svn$"
    ],
    "archivePath": "archive",
    "requires": [
        "etframework"
    ],
    "id": "2cc42d86-158d-43a7-949b-2a48aa71fa5e"
}